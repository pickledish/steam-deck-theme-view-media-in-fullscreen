{
	"name": "View Media in Fullscreen",
	"author": "WorldFS",
	"target": "Media",
	"version": "v1.0",
	"description": "Display the selected image in fullscreen when viewing them in Media page",
	"manifest_version": 5,
	"inject": {
		"shared.css": [
			"SP"
		]
	},
	"dependencies": {},
  "patches": {
    "Pixelated Rendering": {
        "default": "No",
        "type": "checkbox",
        "values": {
            "No": {},
            "Yes": {
              "pixelated.css": ["SP"]
            },
        }
    }
  }
}
